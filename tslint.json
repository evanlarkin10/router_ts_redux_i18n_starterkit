{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "import-name": false,
    "no-console": [
      true,
      "warning"
    ],
    "prefer-array-literal": [true, { "allow-type-parameters": true }],
    "variable-name": [
      false,
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}